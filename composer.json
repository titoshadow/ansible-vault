{
  "name": "titoshadow/ansible-vault",
  "description": "A PHP wrapper for the ansible-vault command.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Titoshadow\\AnsibleVault\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Titoshadow\\AnsibleVault\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "symfony/process": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}